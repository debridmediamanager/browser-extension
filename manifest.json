{
    "manifest_version": 3,
    "name": "Debrid Media Manager",
    "version": "1.8.0",
    "description": "Add DMM buttons to IMDB, MDBList, TraktTV, Letterboxd, JustWatch, TheTVDB, Criticker, Metacritic, and torrent sites",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ],
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },
        "default_title": "Debrid Media Manager"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}